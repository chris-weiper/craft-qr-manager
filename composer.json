{
  "name": "weiperio/craft-qr-manager",
  "description": "Manage your QR code campaigns all in one place. Create branded QR codes and handle redirects intelligently to ensure proper attribution for your campaign sources.",
  "type": "craft-plugin",
  "version": "4.0.0",
  "license": "proprietary",
  "support": {
    "email": "chris.weiper@gmail.com"
  },
  "require": {
    "php": ">=8.0.2",
    "craftcms/cms": "^4.0.0"
  },
  "require-dev": {
    "craftcms/ecs": "dev-main",
    "craftcms/phpstan": "dev-main"
  },
  "autoload": {
    "psr-4": {
      "weiperio\\craftqrmanager\\": "src/"
    }
  },
  "extra": {
    "handle": "qr-manager",
    "name": "QR Manager",
    "developer": "weiperio",
    "documentationUrl": "",
    "class": "weiperio\\craftqrmanager\\QrManager"
  },
  "scripts": {
    "check-cs": "ecs check --ansi",
    "fix-cs": "ecs check --ansi --fix",
    "phpstan": "phpstan --memory-limit=1G"
  },
  "config": {
    "sort-packages": true,
    "platform": {
      "php": "8.0.2"
    },
    "allow-plugins": {
      "yiisoft/yii2-composer": true,
      "craftcms/plugin-installer": true
    }
  }
}